<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proloy.office_management" >



    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"

        android:theme="@style/AppTheme" >
        <activity android:name=".Activity.UI.MainActivity" >
        </activity>
        <activity
            android:name=".Activity.UI.ActivityComplain"
            android:label="@string/title_activity_activity_complain"
            android:parentActivityName=".Activity.UI.MainActivity" />
        <activity
            android:name=".Activity.UI.ActivityNewComplain"
            android:label="New Complain"
            android:parentActivityName=".Activity.UI.ActivityComplain" >
        </activity>


        <activity android:name=".Activity.UI.ActivityNotice"
            android:label="Notice"></activity>
        <activity
            android:name=".Activity.UI.ActivityLogin"
            android:label="Office Management" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.UI.ActivityChatBox"
            android:label="ChatBox"
            android:parentActivityName=".Activity.UI.MainActivity"></activity>
        <activity android:name=".Activity.UI.SplashActivity"></activity>
        <activity android:name=".Activity.UI.ActivityChatLogin"></activity>
        <activity android:name=".Activity.UI.UserListingActivity"></activity>
        <activity android:name=".Activity.UI.ActivityRegister"></activity>

        <meta-data
            android:name="AA_DB_NAME"
            android:value="apto_wizDB.db" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <activity android:name=".Activity.UI.ActivityDrive"></activity>
        <activity android:name=".Activity.UI.ActivityChat"></activity>
        <activity android:name=".Activity.UI.ActivityNumber"></activity>
        <activity android:name=".Activity.Numbers.HeadOfMarketing"></activity>

    </application>

</manifest>
